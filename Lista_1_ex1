package lista_1_ex1;

/**
 *
 * @author LABCCET
 */
public class Lista_1_ex1 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        String[] greeting = new String[3];
        greeting[0] = "Meu primeiro programa em linguagem Java";
        greeting[1] = "Com ajuda do livro Core Java";
        greeting[2] = "Muito bom voltar aImplementar";
        
        for (String g : greeting)
            System.out.println(g);
    }
    
}
